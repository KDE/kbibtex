@phdthesis{normandin2010,
abstract = {The contemporary student veteran population is growing rapidly as service members who served in the Afghanistan and Iraq conflicts take advantage of the Post 9/11 Veteran Educational Assistance Act of 2008 to receive educational benefits to attend college (Cook & Kim, 2009). However, student veterans entering college today have a unique set of qualities and experiences. As an emerging student population, little is known about the experiences and perceptions these student veterans bring with them to college (DiRamio, Ackerman, & Mitchell, 2008). This study explored the nature of contemporary student veteran transition experiences from military life to college life to uncover (a) the factors that had positive effects on the contemporary student veteran transition process, and (b) the factors that posed challenges to the contemporary student veteran transition process. The information presented in this study has significance as it fills a knowledge gap that in existing literature regarding transitional experiences and perceptions of contemporary student veterans. In addition, the findings may serve as a foundation to improve the quality of resources available for those working to serve the needs of this growing student population.},
annote = {this article discusses the unique experiences of veterans transitioning from military service to civilian college environment. The military structure is different than civilian structure so the transition was different. There are also differences in transitions, such as anticipated events, like knowing when military service will end, but also un expected events like injury which will require early discharge from the military and they could not reenlist if they did not transition well into civilain life. The article discusses 4 "S"'s, Situation, Self, Support, Strategy are all neccessary in transitions. Much insight and expected and unexpected findings are found in this thesis, but one of the main focuses is on the supports neccessary and the need to understand the unique situation veteran transition has.},
author = {Normandin, K.},
file = {:C\:/Users/Joe/Misc/Mendeley Desktop/2010/Contemporary student veterans transitional experiences from military life to college life - Normandin.pdf:pdf},
title = {{Contemporary student veterans: transitional experiences from military life to college life}},
year = {2010}
}
