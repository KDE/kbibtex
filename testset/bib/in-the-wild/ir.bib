@TechReport{ baeza05choice,
abstract = {This paper studies a family of link-based algorithms that
propagate page importance through links. In these
algorithms there is a damping function that decreases with
the distance, so a direct link implies more endorsement
than a link through a long path. {PageRank} is the most
widely known ranking function of this family. We focus on
three damping functions, having linear, exponential, and
hyperbolic decay on the lengths of the paths. The
exponential decay corresponds to {PageRank}, and the other
functions are new. Our analysis includes a comparison among
them and experiments for studying their behavior under
different parameters.},
author = {Ricardo Baeza-Yates and Paolo Boldi and Carlos Castillo},
citeulike-article-id={322774},
institution = {Dipartimento di Scienze dell'Informazione, Universit\`{a}
degli Studi di Milano},
