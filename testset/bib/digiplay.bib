@article {,
	title = {Videogames and Art},
	year = {2007},
	note = {<p>
From the <a href="http://www.press.uchicago.edu/cgi-bin/hfs.cgi/00/226001.ctl" target="_blank">publisher\&$\#$39;s website</a>:
</p>
<p>
From <em>Madden NFL 2007</em> to <em>Grand Theft Auto: Vice City</em>,
