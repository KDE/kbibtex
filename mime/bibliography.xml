<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-research-info-systems">
        <sub-class-of type="text/plain"/>
        <alias type="text/x-research-info-systems"/>
        <comment>RIS bibliography file</comment>
        <comment xml:lang="pl">Plik bibliografii RIS</comment>
        <comment xml:lang="pt">Ficheiro de bibliografia do RIS</comment>
        <comment xml:lang="pt_BR">Arquivo de bibliografia RIS</comment>
        <comment xml:lang="sk">Súbor bibliografie RIS</comment>
        <comment xml:lang="sv">RIS-bibliografifil</comment>
        <comment xml:lang="uk">файл бібліографії RIS</comment>
        <acronym>RIS</acronym>
        <expanded-acronym>Research Information Systems</expanded-acronym>
        <glob pattern="*.ris"/>
        <magic priority="90">
            <match type="string" offset="0" value="TY  -"/>
        </magic>
    </mime-type>
    <mime-type type="application/x-isi-export-format">
        <sub-class-of type="text/plain"/>
        <comment>ISI bibliography file</comment>
        <comment xml:lang="pl">Plik bibliografii ISI</comment>
        <comment xml:lang="pt">Ficheiro de bibliografia do ISI</comment>
        <comment xml:lang="pt_BR">Arquivo de bibliografia ISI</comment>
        <comment xml:lang="sk">Súbor bibliografie ISI</comment>
        <comment xml:lang="sv">ISI-bibliografifil</comment>
        <comment xml:lang="uk">файл бібліографії ISI</comment>
        <acronym>ISI</acronym>
        <expanded-acronym>Information Sciences Institute</expanded-acronym>
        <glob pattern="*.isi"/>
        <magic priority="90">
            <match type="string" offset="0" value="FN ISI Export Format"/>
        </magic>
    </mime-type>
    <mime-type type="application/x-endnote-refer">
        <sub-class-of type="text/plain"/>
        <alias type="text/x-endnote-refer"/>
        <alias type="application/x-endnote-library"/>
        <comment>EndNote library</comment>
        <comment xml:lang="pl">Biblioteka EndNote</comment>
        <comment xml:lang="pt">Biblioteca do EndNote</comment>
        <comment xml:lang="pt_BR">Biblioteca EndNote</comment>
        <comment xml:lang="sk">Knižnica EndNote</comment>
        <comment xml:lang="sv">EndNote-bibliotek</comment>
        <comment xml:lang="uk">бібліотека EndNote</comment>
        <glob pattern="*.enl"/>
    </mime-type>
</mime-info>
