<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-research-info-systems">
        <sub-class-of type="text/plain"/>
        <alias type="text/x-research-info-systems"/>
        <comment>RIS bibliography file</comment>
        <comment xml:lang="ca">Fitxer de bibliografia RIS</comment>
        <comment xml:lang="ca@valencia">Fitxer de bibliografia RIS</comment>
        <comment xml:lang="en_GB">RIS bibliography file</comment>
        <comment xml:lang="es">Archivo de bibliografía RIS</comment>
        <comment xml:lang="eu">RIS bibliografia fitxategia</comment>
        <comment xml:lang="fi">RIS-lähdeluettelotiedosto</comment>
        <comment xml:lang="fr">Fichier de bibliographie pour RIS</comment>
        <comment xml:lang="it">File bibliografico RIS</comment>
        <comment xml:lang="nl">RIS-bibliografiebestand</comment>
        <comment xml:lang="nn">RIS-litteraturliste-fil</comment>
        <comment xml:lang="pl">Plik bibliografii RIS</comment>
        <comment xml:lang="pt">Ficheiro de bibliografia do RIS</comment>
        <comment xml:lang="pt_BR">Arquivo de bibliografia RIS</comment>
        <comment xml:lang="sl">RIS datoteka bibliografije</comment>
        <comment xml:lang="sv">RIS-bibliografifil</comment>
        <comment xml:lang="uk">файл бібліографії RIS</comment>
        <comment xml:lang="zh_CN">RIS 参考文献文件</comment>
        <acronym>RIS</acronym>
        <expanded-acronym>Research Information Systems</expanded-acronym>
        <glob pattern="*.ris"/>
        <magic priority="90">
            <match type="string" offset="0" value="TY  -"/>
        </magic>
    </mime-type>
    <mime-type type="application/x-isi-export-format">
        <sub-class-of type="text/plain"/>
        <comment>ISI bibliography file</comment>
        <comment xml:lang="ca">Fitxer de bibliografia ISI</comment>
        <comment xml:lang="ca@valencia">Fitxer de bibliografia ISI</comment>
        <comment xml:lang="en_GB">ISI bibliography file</comment>
        <comment xml:lang="es">Archivo de bibliografía ISI</comment>
        <comment xml:lang="eu">ISI bibliografia fitxategia</comment>
        <comment xml:lang="fi">ISI-lähdeluettelotiedosto</comment>
        <comment xml:lang="fr">Fichier de bibliographie pour ISI</comment>
        <comment xml:lang="it">File bibliografico ISI</comment>
        <comment xml:lang="nl">ISI-bibliografiebestand</comment>
        <comment xml:lang="nn">ISI-litteraturliste-fil</comment>
        <comment xml:lang="pl">Plik bibliografii ISI</comment>
        <comment xml:lang="pt">Ficheiro de bibliografia do ISI</comment>
        <comment xml:lang="pt_BR">Arquivo de bibliografia ISI</comment>
        <comment xml:lang="sl">ISI datoteka bibliografije</comment>
        <comment xml:lang="sv">ISI-bibliografifil</comment>
        <comment xml:lang="uk">файл бібліографії ISI</comment>
        <comment xml:lang="zh_CN">ISI 参考文献文件</comment>
        <acronym>ISI</acronym>
        <expanded-acronym>Information Sciences Institute</expanded-acronym>
        <glob pattern="*.isi"/>
        <magic priority="90">
            <match type="string" offset="0" value="FN ISI Export Format"/>
        </magic>
    </mime-type>
    <mime-type type="application/x-endnote-refer">
        <sub-class-of type="text/plain"/>
        <alias type="text/x-endnote-refer"/>
        <alias type="application/x-endnote-library"/>
        <comment>EndNote library</comment>
        <comment xml:lang="ca">Biblioteca EndNote</comment>
        <comment xml:lang="ca@valencia">Biblioteca EndNote</comment>
        <comment xml:lang="en_GB">EndNote library</comment>
        <comment xml:lang="es">Biblioteca EndNote</comment>
        <comment xml:lang="eu">EndNote liburutegia</comment>
        <comment xml:lang="fi">EndNote-kirjasto</comment>
        <comment xml:lang="fr">Bibliothèque pour EndNote</comment>
        <comment xml:lang="it">Biblioteca EndNote</comment>
        <comment xml:lang="nl">EndNote-bibliotheek</comment>
        <comment xml:lang="nn">EndNote-bibliotek</comment>
        <comment xml:lang="pl">Biblioteka EndNote</comment>
        <comment xml:lang="pt">Biblioteca do EndNote</comment>
        <comment xml:lang="pt_BR">Biblioteca EndNote</comment>
        <comment xml:lang="sl">Knjižnica EndNote</comment>
        <comment xml:lang="sv">EndNote-bibliotek</comment>
        <comment xml:lang="uk">бібліотека EndNote</comment>
        <comment xml:lang="zh_CN">EndNote 库</comment>
        <glob pattern="*.enl"/>
    </mime-type>
</mime-info>
